528278325.html	百度知道/电脑/网络/编程语言/C/C++/	请问 if(argc > 1 && strcmp(argv[1],\"-n\") == 0)与if(argc > 1 && argv[1]==\"-n\")有啥区别？	程序如下（The_C_Programming_Language p121）\r\n#include<stdio.h>\r\n#include<string.h>\r\n#include<stdlib.h>\r\nint numcmp(char *s1,char *s2)\r\n{\r\ndouble v1,v2;\r\nv1 = atof(s1);\r\nv2 = atof(s2);\r\nif(v1 < v2)\r\nreturn -1;\r\nelse if(v1 > v2)\r\nreturn 1;\r\nelse\r\nreturn 0;\r\n}\r\nvoid swap(void *v[],int i,int j)\r\n{\r\nvoid *temp;\r\ntemp = v[i];\r\nv[i] = v[j];\r\nv[j] = temp;\r\n}\r\nvoid qsort(void *v[],int left,int right,int (*comp)(void *,void *))\r\n{\r\nint last,i;\r\nif(left >= right)\r\nreturn;\r\nswap(v,left,(left + right) / 2);\r\nlast = left;\r\nfor(i = left + 1 ; i <= right ; i++){\r\nif(comp(v[left],v[i]) > 0)\r\nswap(v,++last,i);\r\n}swap(v,last,left);\r\nqsort(v,left,last - 1,comp);\r\nqsort(v,last + 1,right,comp);\r\n}\r\n#define MAXLINES 100\r\n#define LINES 100\r\nchar *lineptr[LINES];\r\nint getline(char *s,int lim)\r\n{\r\nint i,c;\r\nchar *t;\r\nt = s;\r\nfor( i = 0 ; i < lim - 1 && (c = getchar()) != EOF && c != '\\n' ; i++)\r\n*s++ = c;\r\nif(c == '\\n')\r\n*s++ = c;\r\n*s = '\\0';\r\nreturn s - t;\r\n}\r\nint readlines(char *v[],int lines)\r\n{\r\nint n,nlines = 0;\r\nchar line[MAXLINES];\r\nchar *p;\r\nwhile( n = getline(line,MAXLINES) > 0){\r\nif(n > lines || (p = (char *)malloc(n)) == NULL)\r\n{\r\nprintf(\"can't alloc\\n\");\r\nreturn -1;\r\n}\r\nelse\r\n{\r\nv[nlines] = p;\r\nstrcpy(v[nlines],line);\r\nnlines++;\r\n}\r\n}return nlines;\r\n}\r\nvoid writelines(char *v[],int nlines)\r\n{\r\nwhile(--nlines >= 0)\r\nprintf(\"%s\",v[nlines]);\r\n}\r\nmain(int argc,char *argv[])\r\n{\r\nint nlines,numeric = 0;\r\nif(argc > 1 && strcmp(argv[1],\"-n\") == 0)/*此处换成if(argc > 1 && argv[1]==\"-n\")，程序输入“-n”numeric也不会=1*/\r\nnumeric = 1;\r\nif((nlines = readlines(lineptr,LINES)) > 0){\r\nqsort((void **)lineptr,0,nlines - 1,(numeric ? (int (*)(void *,void *))numcmp : (int (*)(void *,void *))strcmp));\r\nwritelines(lineptr,nlines);\r\nreturn 0;\r\n}else{\r\nprintf(\"input too big to sort\\n\");\r\nreturn 1;\r\n}\r\n}

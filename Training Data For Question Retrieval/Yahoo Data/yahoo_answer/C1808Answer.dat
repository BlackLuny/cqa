O FIND WHETHER A STRING IS PALIN...     include   include   include   include  typedef struct stack  {  char c;  struct stack *link;  }S;  static int length;    void push(S** top,char ch)  {  S* temp=(S*)malloc(sizeof(S));  temp->c=ch;temp->link=NULL;  if(*top==NULL)  (*top)=temp;    else  {  temp->link=(*top);  (*top)=temp;  }  length++;  }    char pop(S** top)  {  if((*top)==NULL)  {  printf("STack empty");  return NULL;  }  S* temp=(*top);  char ch=temp->c;  (*top)=(*top)->link;  free(temp);  return ch;  }    void inserttostack(S** top,char str[])  {  for(int i=0;i  push(top,str[i]);  }  void isitpalindrome(S** top,char str[])  {  for(int i=0;i  if(str[i]!=pop(top))  {  printf("\n%s:Not Palindrome",str);  return;  }  if(length%2==0)  printf("\n%s: Even Palindrome",str);  else  printf("\n%s: Odd palindrome",str);  }  void display(S* top)  {  while(top!=NULL)  {  printf("%c",top->c);  top=top->link;  }  }    void main()  {  S *top=NULL; char str[20];  printf("Enter a string:");  gets(str);  inserttostack(&top,str);  printf("\n\nstack length: %d\nSTACK:  ",length);display(top);  isitpalindrome(&top,str);  }
